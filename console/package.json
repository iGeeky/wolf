{
  "name": "wolf-console",
  "version": "0.2.0",
  "author": "igeeky.io@gmail.com",
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build --force && mkdir -p ../server/html && rm -Rf ../server/html/* && cp -R dist/* ../server/html/",
    "build:stage": "vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
    "new": "plop"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "wolf-console",
    "rbac",
    "console"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iGeeky/wolf.git"
  },
  "bugs": {
    "url": "https://github.com/iGeeky/wolf/issues"
  },
  "dependencies": {
    "axios": ">=0.21.1",
    "element-ui": "2.7.0",
    "fuse.js": "3.4.4",
    "gojs": "^2.0.16",
    "js-cookie": "2.2.0",
    "moment": "^2.24.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "^6.1.0",
    "screenfull": "4.2.0",
    "vue": "2.6.10",
    "vue-json-viewer": "^2.2.8",
    "vue-router": "3.0.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/register": "7.0.0",
    "@vue/cli-plugin-babel": "3.5.3",
    "@vue/cli-plugin-eslint": "^3.9.1",
    "@vue/cli-service": "3.5.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.15.3",
    "eslint-plugin-vue": "^5.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "node-sass": "^4.13.0",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "svg-sprite-loader": "4.1.3",
    "vue-template-compiler": "2.6.10"
  },
  "engines": {
    "node": ">=8.9",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
